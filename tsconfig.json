{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "routes/*": ["./src/routes/*"],
      "screens/*": ["./src/screens/*"],
      "services/*": ["./src/services/*"],
      "utils/*": ["./src/utils/*"]
    },
    "jsx": "react-native",
    "lib": [ "dom", "esnext" ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": false,
    "strictNullChecks": false
  },
  "exclude": [ "node_modules" ],
  "include": [ "./**/*.*.ts", "./**/*.*.tsx" ]
}